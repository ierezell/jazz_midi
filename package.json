		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.5.0",
		"happy-dom": "^20.0.11",
		"jsdom": "^27.2.0",
		"prettier": "3.7.1",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "5.45.2",
		"svelte-check": "4.3.4",
		"typescript": "5.9.3",
		"typescript-eslint": "8.48.0",
		"vite": "7.2.4",
		"vitest": "^4.0.14"
	},
